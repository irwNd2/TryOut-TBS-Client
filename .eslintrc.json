{
	"root": true,
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:vue/vue3-essential", "prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["vue"],
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": 0,
		"quotes": ["error", "single"],
		"semi": ["error", "never"],
		"no-unused-vars": ["error"]
	}
}
